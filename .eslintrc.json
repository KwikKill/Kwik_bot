{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "for-direction": "error",
        "getter-return": "error",
        "no-cond-assign": "error",
        "no-const-assign": "error",
        "no-constant-condition": "error",
        "no-dupe-else-if": "error",
        "no-duplicate-case": "error",
        "no-loss-of-precision": "error",
        "no-setter-return": "error",
        "no-sparse-arrays": "error",
        "no-unreachable": "error",
        "no-unreachable-loop": "error",
        "no-unused-vars": "error",
        //"complexity": "error",
        "semi": "error",
        //"no-await-in-loop": "error",
        "eqeqeq": "error",
        "no-console": [
            "warn",
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ],
        "no-else-return": "warn",
        "no-magic-numbers": [
            "warn",
            {
                "ignoreArrayIndexes": true,
                "ignoreDefaultValues": true,
                "ignore": [
                    0,
                    -1,
                    1,
                    200,
                    400,
                    404,
                    429,
                    10,
                    5,
                    100,
                    60,
                    0.2,
                    0.99,
                    4,
                    2,
                    3
                ]
            }
        ],
        "no-var": "error",
        "curly": "error",
        "prefer-const": "error",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "jsx-quotes": [
            "error",
            "prefer-double"
        ],
        "no-trailing-spaces": "error",
        "no-mixed-spaces-and-tabs": [
            "error",
            "smart-tabs"
        ],
        "no-irregular-whitespace": "error",
        "no-multi-spaces": "error",
        "no-empty": [
            "error",
            {
                "allowEmptyCatch": true
            }
        ]
        //"camelcase": "warn"
    },
    "extends": [
        "eslint:recommended"
    ],
    "ignorePatterns": [
        "commands/eval.js"
    ]
}